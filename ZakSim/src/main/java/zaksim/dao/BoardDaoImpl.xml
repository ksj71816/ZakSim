<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="zaksim.dao.BoardDao"> 
  
  <select id="selectSeq" resultType="int">
  
 	 select board_seq.nextval from dual
  
  </select>
  
  
  <insert id="insertCitation" parameterType="zaksim.dto.Board">
  
  	INSERT INTO BOARD VALUES (#{idx}, #{challenge_idx}, null, #{writer_idx}, #{title}, #{content}, #{image}, 1, #{certification}, sysdate, #{hit})
  
  </insert>
  
  
  
  
  
  
  
  
  
  
  
  </mapper>
  