<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="zaksim.dao.CommunityListDAO">
  
    <select id="joinedGroupList" resultType="zaksim.dto.CommunityGroup" >
  			select * from CommunityGroup order by idx asc
  </select>
  
  <select id="categoryList" resultType="zaksim.dto.CommunityCategory" >
  			select * from CommunityCategory order by idx asc
  </select>
  
  </mapper>